.menu__items{
    display: flex;   
    @include tablets{
        display: flex;
     flex-wrap: wrap;
    }
}
.menu__link{
    color: #464d62;
    font-family: "Open Sans";
    font-size: 16px;
    font-weight: 400;
    &:hover{
        transition: 0.6s;
        color: #5500f2;
    }
}
.menu__item{
    margin-right:62px;
    &:last-child{
        margin-right:0;
    }
}